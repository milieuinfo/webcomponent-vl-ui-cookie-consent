{
  "name": "vl-ui-cookie-consent",
  "version": "1.0.0",
  "main": "index.js",
  "description": "De cookie consent kan gebruikt worden om de gebruiker te informeren over de cookies die gebruikt worden.",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "pretest": "npm install chromedriver --chromedriver-force-download",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "wct --config-file ./wct.local.conf.json",
    "test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
    "test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
    "posttest:e2e": "pkill -f http-server",
    "test:e2e:chrome": "mocha --chrome ",
    "test:e2e:chrome:grid": "mocha  --chrome --grid",
    "test:e2e:firefox": "mocha --firefox",
    "test:e2e:firefox:grid": "mocha --firefox --grid",
    "test:e2e:opera": "mocha --opera",
    "test:e2e:safari": "mocha --safari",
    "test:server:start": "http-server &",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-cookie-consent.js -- *.src.js",
    "release:prepare:imports": "npm run release:prepare:imports:core && npm run release:prepare:imports:src",
    "release:prepare:imports:core": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-cookie-consent.js",
    "release:prepare:imports:src": "replace '/src/analytics.js' '/node_modules/vl-ui-cookie-consent/src/analytics.js' vl-cookie-consent.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-cookie-consent/style.css' vl-cookie-consent.js",
    "release:prepare:commit": "git add -f vl-cookie-consent.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-cookie-consent.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-cookie-consent/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-cookie-consent#readme",
  "dependencies": {
    "vl-ui-button": "^1.3.2",
    "vl-ui-checkbox": "github:milieuinfo/webcomponent-vl-ui-checkbox#UIG-271",
    "vl-ui-core": "^2.2.5",
    "vl-ui-form-grid": "^1.0.12",
    "vl-ui-form-message": "^3.0.0",
    "vl-ui-modal": "^1.0.15"
  },
  "peerDependencies": {
    "vl-ui-button": "^1.3.1",
    "vl-ui-checkbox": "github:milieuinfo/webcomponent-vl-ui-checkbox#UIG-271",
    "vl-ui-form-message": "^3.0.0",
    "vl-ui-form-grid": "^1.0.12",
    "vl-ui-modal": "^1.0.14"
  },
  "devDependencies": {
    "@govflanders/vl-ui-util": "^3.9.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chromedriver": "78.0.1",
    "geckodriver": "1.19.1",
    "http-server": "0.12.0",
    "mocha": "6.2.2",
    "np": "3.0.4",
    "replace": "^1.1.1",
    "selenium-webdriver": "4.0.0-alpha.5",
    "uglify-js-es6": "2.8.9",
    "vl-ui-util": "^3.0.10",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2",
    "yargs": "15.0.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
